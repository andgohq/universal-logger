{
  "name": "@andgohq/universal-logger",
  "version": "1.16.1",
  "description": "Logger for browser, nodejs and electron",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "browserslist": [
    "Chrome > 66"
  ],
  "engines": {
    "node": ">=14.x"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=14.x",
        "electron": ">=2.x",
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "scripts": {
    "dev:web": "parcel serve --dist-dir tests/dist tests/index.html",
    "dev:nodejs": "node --enable-source-maps -r @swc-node/register tests/runner-nodejs.ts",
    "build": "parcel build",
    "clean": "rm -rf dist tests/*.js"
  },
  "author": "AndGo, Inc.",
  "license": "MIT",
  "repository": "git://github.com/andgohq/universal-logger.git",
  "prettier": "@andgohq/prettier-config",
  "dependencies": {
    "chalk": "^4.1.2",
    "dayjs": "^1.11.4",
    "mask-json": "^1.0.3",
    "pino": "^7.9.2"
  },
  "devDependencies": {
    "@andgohq/eslint-config-ts": "^1.6.0",
    "@andgohq/prettier-config": "^1.0.1",
    "@parcel/packager-ts": "^2.4.0",
    "@parcel/transformer-typescript-types": "^2.4.0",
    "@swc-node/register": "^1.4.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "buffer": "^6.0.3",
    "eslint": "^8.12.0",
    "parcel": "^2.4.0",
    "prettier": "^2.6.1",
    "process": "^0.11.10",
    "typescript": "4.6.3"
  }
}
