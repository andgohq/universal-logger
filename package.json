{
  "name": "@andgohq/universal-logger",
  "version": "1.9.0",
  "description": "Logger for browser, nodejs and electron",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/index.d.ts",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "engines": {
    "node": ">=14.x"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=14.x",
        "electron": ">=2.x",
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "dev:nodejs": "ts-node src/runner-nodejs.ts",
    "build": "rm -rf dist && parcel build src/index.ts",
    "clean": "rm -rf .parcel-cache dist"
  },
  "author": "AndGo, Inc.",
  "license": "MIT",
  "repository": "git://github.com/andgohq/universal-logger.git",
  "prettier": "@andgohq/prettier-config",
  "dependencies": {
    "date-fns": "^2.23.0",
    "pino": "^7.0.0-rc.3",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@andgohq/eslint-config-ts": "^1.4.0",
    "@andgohq/prettier-config": "^1.0.1",
    "@parcel/packager-ts": "^2.0.0-rc.0",
    "@parcel/transformer-typescript-types": "^2.0.0-rc.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "eslint": "^7.32.0",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "4.3.5"
  }
}
