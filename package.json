{
  "name": "@andgohq/universal-logger",
  "version": "1.7.0",
  "description": "Logger for brower, nodejs and electron",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/index.d.ts",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "engines": {
    "node": ">=4.x"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=4.x",
        "electron": ">=2.x",
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "dev:nodejs": "ts-node src/runner-nodejs.ts",
    "build": "rm -rf dist && parcel build src/index.ts",
    "clean": "rm -rf .parcel-cache dist"
  },
  "author": "AndGo, Inc.",
  "license": "MIT",
  "repository": "git://github.com/andgohq/universal-logger.git",
  "prettier": "@andgohq/prettier-config",
  "peerDependencies": {
    "pino-pretty": "^4.5.0"
  },
  "dependencies": {
    "@datadog/browser-logs": "^2.5.2",
    "date-fns": "^2.17.0",
    "pino": "^6.11.1"
  },
  "devDependencies": {
    "@andgohq/eslint-config-ts": "^1.2.0",
    "@andgohq/prettier-config": "^1.0.1",
    "@types/pino": "^6.3.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "eslint": "^7.19.0",
    "parcel": "^2.0.0-nightly.506",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
