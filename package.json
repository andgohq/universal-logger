{
  "name": "@andgohq/universal-logger",
  "version": "1.10.3",
  "description": "Logger for browser, nodejs and electron",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "engines": {
    "node": ">=14.x"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=14.x",
        "electron": ">=2.x",
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "scripts": {
    "dev:web": "parcel serve --dist-dir tests/dist tests/index.html",
    "dev:nodejs": "ts-node tests/runner-nodejs.ts",
    "build": "rm -rf dist && rollup -c",
    "clean": "rm -rf dist tests/*.js"
  },
  "author": "AndGo, Inc.",
  "license": "MIT",
  "repository": "git://github.com/andgohq/universal-logger.git",
  "prettier": "@andgohq/prettier-config",
  "dependencies": {
    "date-fns": "^2.23.0",
    "pino": "^7.0.0-rc.3",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@andgohq/eslint-config-ts": "^1.4.0",
    "@andgohq/prettier-config": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "esbuild": "^0.12.25",
    "eslint": "^7.32.0",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.3.2",
    "rollup": "^2.56.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "ts-node": "^10.2.1",
    "typescript": "4.4.2"
  }
}
