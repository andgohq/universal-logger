{
  "name": "@andgohq/universal-logger",
  "version": "1.13.3",
  "description": "Logger for browser, nodejs and electron",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "engines": {
    "node": ">=14.x"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=14.x",
        "electron": ">=2.x",
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "scripts": {
    "dev:web": "parcel serve --dist-dir tests/dist tests/index.html",
    "dev:nodejs": "node -r @swc-node/register tests/runner-nodejs.ts",
    "build": "rm -rf dist && rollup -c",
    "clean": "rm -rf dist tests/*.js"
  },
  "author": "AndGo, Inc.",
  "license": "MIT",
  "repository": "git://github.com/andgohq/universal-logger.git",
  "prettier": "@andgohq/prettier-config",
  "dependencies": {
    "@swc-node/register": "^1.4.2",
    "chalk": "^4.1.2",
    "date-fns": "^2.28.0",
    "pino": "^7.9.1",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@andgohq/eslint-config-ts": "^1.6.0",
    "@andgohq/prettier-config": "^1.0.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "esbuild": "^0.14.27",
    "eslint": "^8.11.0",
    "parcel": "^2.3.2",
    "prettier": "^2.6.0",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "typescript": "4.6.2"
  }
}
